<!-- TAGS LIST -->
<body class="page">
    <header>
        <h1>
            <%- page.title %>
        </h1>
    </header>

    <hr>

    <main>
        <%- page.content %>

        <%- site.tags.length ? site.tags.data.map(t => `<a href="${url_for(t.path)}">${t.name}</a>`).join('<br>') : 'None'%>
    </main>

    <hr>

    <footer>
        <%- partial('partials/site-footer') %>
    </footer>

</body>

